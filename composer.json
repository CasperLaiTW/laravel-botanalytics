{
    "name": "casperlaitw/laravel-botanalytics",
    "description": "A Botanalytics Wrapper for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel-botanalytics",
        "botanalytics",
        "bot",
        "analytics"
    ],
    "authors": [
        {
            "name": "Casper Lai",
            "email": "casper.lai@sleepingdesign.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "casperlaitw/botanalytics-php": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "nesbot/carbon": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Casperlaitw\\LaravelBotanalytics\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Casperlaitw\\LaravelBotanalytics\\Tests\\": "tests/"
        }
    }
}
